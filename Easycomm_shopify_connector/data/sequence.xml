<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Sequence for Inventory Sync -->
    <record id="seq_shopify_inventory_sync" model="ir.sequence">
        <field name="name">Shopify Inventory Sync</field>
        <field name="code">shopify.inventory.sync</field>
        <field name="prefix">INV/SYNC/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Sequence for Sync Logs -->
    <record id="seq_shopify_sync_log" model="ir.sequence">
        <field name="name">Shopify Sync Log</field>
        <field name="code">shopify.sync.log</field>
        <field name="prefix">SYNC/LOG/</field>
        <field name="padding">6</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
